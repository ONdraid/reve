{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "reve-gui",
    "version": "0.1.8"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/reve-gui-icon-32x32.png",
        "icons/reve-gui-icon-128x128.png",
        "icons/reve-gui-icon-128x128@x2.png",
        "icons/reve-gui-icon.icns",
        "icons/reve-gui-icon.ico"
      ],
      "identifier": "com.oloko64.reve-gui",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [
        "./models/**/*",
        "./resources/**/*"
      ],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 573,
        "resizable": false,
        "title": "reve-gui",
        "width": 870
      }
    ]
  }
}
